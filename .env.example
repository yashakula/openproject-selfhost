# OpenProject Configuration
# Copy this file to .env and fill in your values

# Generate a secure random key using: openssl rand -hex 64
SECRET_KEY_BASE=

# Your Cloudflare Tunnel domain (e.g., openproject.yourdomain.com)
OPENPROJECT_HOST=

# Cloudflare Tunnel Token (get this from Cloudflare Zero Trust dashboard)
CLOUDFLARE_TUNNEL_TOKEN=
